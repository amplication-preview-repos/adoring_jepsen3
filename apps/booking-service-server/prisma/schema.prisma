datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  bookingDate DateTime?
  createdAt   DateTime  @default(now())
  event       String?
  id          String    @id @default(cuid())
  updatedAt   DateTime  @updatedAt
  user        String?
}
